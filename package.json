  "devDependencies": {
    "@types/node": "22.10.2",
    "@vitest/coverage-v8": "2.1.8",
    "eslint": "9.17.0",
    "eslint-plugin-jsdoc": "50.6.1",
    "hyparquet-compressors": "1.0.0",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.1",
    "vitest": "2.1.8"
  }
